{
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "commonjs": true,
      "node": true,
      "browser": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": 0
    },
    "globals": {
      "ReactDOM": true,
      "React": true,
      "_": true
    }
}