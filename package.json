{
  "name": "coding-challenge-backend-c",
  "version": "0.0.0",
  "description": "coding challenge in Nodejs",
  "main": "server/index.js",
  "scripts": {
    "start": "node server",
    "dev": "nodemon server",
    "debug": "node --inspect server",
    "test": "./node_modules/mocha/bin/mocha ./test/",
    "loadtest": "env NODE_ENV=ci node server",
    "benchmark": "siege -R ./test/.siegerc -f ./test/urls.txt",
    "benchmark-cached": "siege -R ./test/.siegerc -f ./test/urls-with-cache.txt",
    "dev-ui": "./node_modules/.bin/babel --presets react ./frontend --out-dir ./public",
    "postinstall": "npm run dev-ui"
  },
  "author": "Steve",
  "license": "WTFPL",
  "devDependencies": {
    "chai": "^1.9.1",
    "mocha": "^1.18.2",
    "supertest": "^0.11.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "express": "4.15.4",
    "memored": "^1.1.1"
  },
  "engines": {
    "node": "8.3.0"
  }
}
